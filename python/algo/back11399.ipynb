   "source": [
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "arr.sort()\n",
    "\n",
    "result=0\n",
    "for i in range(n) :\n",
    "    result += arr[i]*(n-i)\n",
    "\n",
    "print(result)"
   ]
